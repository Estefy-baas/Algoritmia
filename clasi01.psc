Algoritmo clasi01
	//Entrada: numeroEntero
	//Salida: clasificacion
	//Casos de prueba: 0, -5, 6
	
	Definir numeroEntero como entero;
	
	Escribir "Ingrese un numero entero";
	Leer numeroEntero;
	
	Si numeroEntero<0 Entonces;
		Escribir "negativo " numeroEntero;
	
	Sino 
		si numeroEntero> 0 entonces;
			Escribir  "Positivo " numeroEntero;
		sino 
			Escribir "cero " numeroEntero;
		FinSi
	FinSi
	
FinAlgoritmo
